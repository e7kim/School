	public static int binarySearch(citystate[] array, int entry)
	{	
		int L = 0, R = array.length-1;
		int compare;
		while(L <= R)
		{
			int M = L + (R-L)/2;
			compare = array[M].getCity().compareTo(array[entry].getState());
			if (compare == 0)
				compare = array[M].getState().compareTo(array[entry].getState());
			if (compare == 0)
			{
				return M;
			}
			else if (compare < 0)
			{
				L = M + 1;
			}
			else if (compare > 0)
			{
				R = M - 1;
			}
		}
		return -1;
	}
