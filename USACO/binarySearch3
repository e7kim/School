public static int binarySearch(citystate[] array, int i)
	{	
		int L = 0, R = array.length-1;
		int M = -1;
		while(L <= R)
		{
			M = L + (R-L)/2;
			if (array[M].compareTo2(array[i]) == 0 && M != i)
			{
				return M;
			}
			else if (array[M].compareTo2(array[i]) < 0)
			{
				L = M + 1;
			}
			else if (array[M].compareTo2(array[i]) > 0)
			{
				R = M - 1;
			}
			else if (array[M].compareTo2(array[i]) == 0 && M == i)
				return -1;
			System.out.println(M);
		}
		return -1;
	}

}
